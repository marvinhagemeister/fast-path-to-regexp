{
  "name": "@marvinh/path-to-regexp",
  "version": "0.0.5",
  "description": "Ultra small (350b) and blazing fast path to regex generation",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "author": "Marvin Hagemeister <marvin@marvinhagemeister.de>",
  "license": "MIT",
  "scripts": {
    "cleanup": "rimraf dist/",
    "lint": "tslint 'src/**/*.ts' 'src/**/*.tsx'",
    "watch": "tsc -p tools/tsconfig.build.json -w",
    "build": "npm run cleanup && tsc -p tools/tsconfig.build-cjs.json && tsc -p tools/tsconfig.build-es.json",
    "test": "jest",
    "test-watch": "npm t -- --watch",
    "coverage": "npm t -- --coverage",
    "prepublishOnly": "npm run lint && npm t && npm run build",
    "bench": "ts-node benchmark/index.ts",
    "precommit": "lint-staged"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "benchmarkjs-pretty": "^2.0.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "path-to-regexp": "^2.4.0",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.1",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-sevenval": "^2.0.0",
    "typescript": "^3.1.3",
    "url-pattern": "^1.0.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --trailing-comma=all --write",
      "npm run lint",
      "git add"
    ]
  }
}
